{
  "name": "vue-blog",
  "version": "1.0.0",
  "description": "- vue-cli v4.2.3\r - Node.js v12.13.0\r - MongoDB v4.2.17",
  "scripts": {
    "serve": "run-p web-serve admin-serve server-serve",
    "server-serve": "cd server && npm run serve",
    "web-serve": "cd web && npm run serve",
    "admin-serve": "cd admin && npm run serve",
    "install": "run-p web-install admin-install server-install",
    "server-install": "cd server && npm install",
    "web-install": "cd web && npm install",
    "admin-install": "cd admin && npm install",
    "build": "run-p web-build admin-build",
    "web-build": "cd web && npm run build",
    "admin-build": "cd admin && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqilin21/vue-blog.git"
  },
